       "       and dwlmes_dbt.t_sessionid=dwlsess_dbt.t_sessionid  "+
       "       and dwlpm_dbt.t_PaymentID="+id;

   data=TRsbDataSet(s);
   data.movenext();
   return data.bankdate;
end;

