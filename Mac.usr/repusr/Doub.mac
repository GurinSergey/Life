   if (flag)
      SaveBeginTime();//‘â àâ
      ArchDate = {Curdate} - 1;
      OutAlld(id);
      SaveEndTime(2); //‡ ¢¥àè¨«¨
   else
      if (RunDialog(dlg, "Event"))                  
         SaveBeginTime();//‘â àâ
         OutAlld;
         ViewFile(nameout);
         SaveEndTime(2); //‡ ¢¥àè¨«¨
      end; 
   end;
end;

