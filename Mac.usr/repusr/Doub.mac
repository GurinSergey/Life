MACRO Event (dlg, cmd, id, key) 

   var const_mess = " ~F3~ ‚ë¡®à ¤ âë ¨§ ª «¥­¤ àï ~F2~ à®¤®«¦¨âì ~ESC~ ‚ëå®¤ ";

   /*¥à¢®­ ç «ì­ ï ¨­¨æ¨ «¨§ æ¨ï ¯®«¥©*/
   if(cmd == DLG_INIT)
      dlg.rec.ArchDate = {curDate}-1;
      UpdateFields(dlg); 
   end;
   
   /*“áâ ­®¢ª  ¯®¤áª §®ª ¢ áâà®ª¥ á®áâ®ï­¨ï*/
   if (cmd==DLG_SETFOCUS)
      message(const_mess);
   end;
   
   if (cmd == DLG_REMFOCUS)
      /*à®¢¥àª  ª®àà¥ªâ­®áâ¨ ¤ â ®âç¥â */
      if (FldName(dlg,id) == "ArchDate")
         if ( dlg.rec.ArchDate >= {curdate} )
            MsgBox("€àå¨¢­ ï ¤ â  ¤®«¦­  ¡ëâì ¬¥­ìè¥ ¤ âë â¥ªãé¥£® ®¯¥à æ¨®­­®£® ¤­ï");
            return CM_CANCEL;
         end;
      end;
         UpdateFields(dlg); 
      end;

      if (cmd == DLG_KEY)
         /*‚ëå®¤ ¨§ ¤¨ «®£®¢®£® ®ª­  ä®à¬¨à®¢ ­¨ï ®âç¥â */
         if (KEY == KEY_ESC)
            return exit(1);//CM_CANCEL;
         /*‚ë¡®à ¤ ­­ëå ¨§ á¯¨áª */
         elif ( KEY == KEY_F3)
            /*‚ë¡®à ¤ âë ¨§ ª «¥­¤ àï*/
            if (FldName(dlg,id) == "ArchDate")
               dlg.rec.ArchDate = GetDateByCalendar ({curDate}-1);
               if (dlg.rec.ArchDate>= {curdate} )
                  MsgBox("€àå¨¢­ ï ¤ â  ¤®«¦­  ¡ëâì ¬¥­ìè¥ ¤ âë â¥ªãé¥£® ®¯¥à æ¨®­­®£® ¤­ï");
                  dlg.rec.ArchDate= {curdate}-1;
                  return CM_IGNORE;
               end;
            end;
        
         elif (( KEY == KEY_F2 ) or (KEY == KEY_ENTER))         //à®¢¥àª¨ ¯à¨ ¢¢®¤¥
            if ( dlg.rec.ArchDate >= {curdate} )
               MsgBox("€àå¨¢­ ï ¤ â  ¤®«¦­  ¡ëâì ¬¥­ìè¥ ¤ âë â¥ªãé¥£® ®¯¥à æ¨®­­®£® ¤­ï");
               return CM_IGNORE;
            end;
            ArchDate  = dlg.rec.ArchDate;
            Return CM_SAVE;
         end;
      else
           Return CM_IGNORE;
      end;
END;

