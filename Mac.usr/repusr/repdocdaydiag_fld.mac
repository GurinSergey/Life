macro date_format(_date)
	var _day, _mon, _year;
	datesplit(_date, _day, _mon, _year);
	if (_mon == 1)
		_mon = "января";
	elif (_mon == 2)
		_mon = "февраля";
	elif (_mon == 3)
		_mon = "марта";
	elif (_mon == 4)
		_mon = "апреля";
	elif (_mon == 5)
		_mon = "мая";
	elif (_mon == 6)
		_mon = "июня";
	elif (_mon == 7)
		_mon = "июля";
	elif (_mon == 8)
		_mon = "августа";
	elif (_mon == 9)
		_mon = "сентября";
	elif (_mon == 10)
		_mon = "октября";
	elif (_mon == 11)
		_mon = "ноября";
	elif (_mon == 12)
		_mon = "декабря";
	end;

	return (_day + " " + _mon + " " + _year);
end;




macro openExcel()
	var out;
   if (IsStandAlone()) // 2х звенка
      if ((ValType(ex)!=V_UNDEF) and (ex.Visible==False))
      	ex = ActiveX("Excel.Application",NULL,false); 
      else
      	ex = ActiveX("Excel.Application",NULL,true);        
      end;
   else                // 3х звенка
      ob = CreateObject ("rsax","TRsAxServer","RsAxServer",false);
      ex = ob.CreateComObject ("Excel.Application");
   end; 
   debugbreak;	
   DecimalSeparator = ex.DecimalSeparator;

   obBook = ex.Workbooks.add; 
   obSheet = obBook.ActiveSheet(); 
	
end;



