    + "EXCEPTION WHEN v_err THEN NULL; \n"
    + "END; \n";
  SQL_Execute(sql,"Создание таблицы отчета", false);
  /*Очищаем таблицу, если есть*/
  sql = "DECLARE \n"
    +  " v_err EXCEPTION; \n"
    +  " PRAGMA EXCEPTION_INIT( v_err, -955 );\n"
    + "BEGIN EXECUTE IMMEDIATE 'truncate table "+table_name+"';\n " 
    + "EXCEPTION WHEN v_err THEN NULL; \n"
    + "END; \n";
  SQL_Execute(sql,"Удаление таблицы отчета", false);
  return 1;
