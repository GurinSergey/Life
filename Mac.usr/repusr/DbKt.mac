     if (not RunDialog(dlg, "Event"))                  
        return 0;
     end;
  end;
  //Tikh Пробуем так  
  /*Создаем таблицу, если таблицы нет*/
  sql = "DECLARE \n"
    +  " v_err EXCEPTION; \n"
    +  " PRAGMA EXCEPTION_INIT( v_err, -955 );\n"
    + "BEGIN EXECUTE IMMEDIATE 'CREATE TABLE "+table_name+" \n"
    + " (T_PaymentID NUMBER(10),  \n" 
