private macro lpad(s,c)
  return mkstr( " ", c - strlen(string(s)) ) + string(s);
end;

private macro rpad(s,c)
  return string(s) + mkstr( " ", c - strlen(string(s)) );
end;

private macro nvl(s)
  if (StrLen(s) > 0)
     return s;
  else
     return " ";
  end;
end;

