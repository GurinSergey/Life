      /*Обработчик диалоговой панели ввода исходных данных для печати*/
   MACRO Event (dlg, cmd, id, key)
  
              
   var mess = "~F2~ Продолжить  ~ESC~ Выход ";
        if (cmd == DLG_INIT)
         dlg.rec.Acc       = Acc;   
         dlg.rec.date_report = {curdate};
         UpdateFields(dlg); 
      end;
      
      if (cmd==DLG_SETFOCUS)
        message(mess);        
      end;
      
      if (cmd == DLG_REMFOCUS)
        if (FldName(dlg,id)=="Acc")
           if (StrLen(dlg.rec.acc) < 20)
              MsgBox("Длина счета менее 20 символов");
              return CM_CANCEL;
           elif (not GetAccount(StrSubst(dlg.rec.acc, "'", "")))
              MsgBox("Счет отсутствует в справочнике лицевых счетов");
              return CM_CANCEL;
           else
              Client = GetClientByAccount(dlg.rec.acc, Chapter, Code);
              Branch = GetBranchByAccount(dlg.rec.acc, Chapter, Code);
           end;
        elif (FldName(dlg,id)=="date_report")
           if ( dlg.rec.date_report > {curdate} )
              MsgBox("Дата отчета больше даты текущего операционного дня"); 
              return CM_CANCEL;
           end;
        end;
        UpdateFields(dlg); 
      end; 

      if (cmd==DLG_KEY)
        if (KEY==KEY_ESC)
           return CM_CANCEL;
                                   
        elif ( KEY==KEY_F2 )
           Account = dlg.rec.acc;
           data_report = dlg.rec.date_report;             
        /*   var fgBank = fg_life_subject({OurBank}); // KS 10.08.2010 I-058375 Шаблон ДБК нужен только для ПРББ  
  
             
           if ((dlg.rec.button == "X") and ((fgBank.is_PRBB) or (fgBank.is_GEB)))
                Templ = "RefAbs2Dkb.dot";
            else
                Templ = "RefAbs2.dot" ;   
                end;*/
              
           if (strlen(string(data_report)) == 0)
             MsgBox("Не указана дата отчета");
             Setfocus(dlg,1);
             return CM_IGNORE;
           end;

           /*Отчет печатается при корректных параметрах*/
           if ((strlen(Account) == 20) and (GetAccount(StrSubst(dlg.rec.acc, "'", ""))) 
              and (strlen(string(data_report)) != 0) and ( dlg.rec.date_report <= {curdate} ))
             
             Return CM_SAVE;
           else
             
             MsgBox("Не все поля заполнены или корректны!");
           end;

       elif (KEY==KEY_SPACE)
            if(FldName(dlg,id)=="button")
                if (dlg.rec.button == "")
                    dlg.rec.button = "X";
                else
                    dlg.rec.button = "";
                end;
            end;
        elif ( (KEY==KEY_ENTER) and (FldName(dlg,id)=="date_report") )
           SetFocus(dlg, 0);
           Return CM_IGNORE;
        end;
      end;
   END;

  if ( RunDialog(dlg, "Event"))
     PrintSprAcc();
  end;
exit(1);

