/*Для запуска модуля использовать данный макрос*/
//import ;
 array arr;
 var choice;
  arr(0)="Без вывода в Excell";
  arr(1)="С выводом в Excell";

 choice = menu(arr,"Выпуск отчета","Какой отчет выпускаем?");
 if(choice < 0)
    exit(1);
 elif(choice == 0)
    execmacrofile("fm_terr.mac","fm_run");
 else
    execmacrofile("fm_terr_xls.mac","fm_run_xls");
 end;
// fm_run();

 end;