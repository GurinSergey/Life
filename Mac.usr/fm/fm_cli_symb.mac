MACRO GetRAcc(partyid)
//Lavrenov: 11.07.2012 в рамках перехода на 11G к вызову wm_concat добавляем dbms_lob.substr
/* EVG 30/1/2014 Переход на 2031.
        var sqlRAcc = " SELECT  dbms_lob.substr( NVL(wm_concat (ACC.T_ACCOUNT), ' ')) racc " 
+"\n  FROM   (SELECT   * " 
+"\n            FROM   daccount_dbt " 
+"\n           WHERE       T_TYPE_ACCOUNT LIKE '%Ч%' " 
+"\n                   AND T_OPEN_CLOSE <> 'З' " 
+"\n                   AND T_CHAPTER = 1 " 
+"\n          UNION ALL " 
+"\n          SELECT   * " 
+"\n            FROM   daccount$_dbt " 
+"\n           WHERE       T_TYPE_ACCOUNT LIKE '%X%' " 
+"\n                   AND T_OPEN_CLOSE <> 'З' " 
+"\n                   AND T_CHAPTER = 1) acc " 
+"\n WHERE   ACC.T_CLIENT = " + partyid;*/

        var sqlRAcc = " SELECT  dbms_lob.substr( NVL(wm_concat (ACC.T_ACCOUNT), ' ')) racc " 
                     +"\n  FROM   (SELECT * " 
                     +"\n            FROM daccount_dbt " 
                     // EVG 30/1/2014 Можно написать так, т.к. руб. и вал. типы счетов не пересекаются.
                     // LVV 12/11/2014 А вот и пересеклись после перевода ЭВ на 2031. Завел условие ИЛИ в скобки.
                     +"\n           WHERE (T_TYPE_ACCOUNT LIKE '%Ч%' OR T_TYPE_ACCOUNT LIKE '%X%') " 
                     +"\n             AND T_OPEN_CLOSE <> 'З' " 
                     +"\n             AND T_CHAPTER = 1) acc " 
                     +"\n WHERE   ACC.T_CLIENT = " + partyid;

        var rsRAcc = trsbdataset(sqlRAcc);
        if (rsRAcc and rsRAcc.movenext)
                return rsRAcc.racc;
        else
                return "";
        end;
end;

