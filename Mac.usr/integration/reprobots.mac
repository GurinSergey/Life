    GetRegistryValue("PRBB\\МОНИТОР ОБРАБОТЧИКОВ\\NORMA_COUNT\\"+nameRobot, V_INTEGER, normaCount, errCode); // debugbreak;

    if ( errCode != 0)
       GetRegistryValue("PRBB\\МОНИТОР ОБРАБОТЧИКОВ\\NORMA_COUNT_DEFAULT", V_INTEGER, normaCount, errCode);	
    end;

    if ( errCode == 0 )
       [ Нормальное количество обработчиков:## шт.](normaCount); 
       if ( normaCount > countRobots ) 
          [ Внимание! Количество активных обработчиков ниже нормы на ## шт.](normaCount - countRobots);
       end;
    end;

END;

