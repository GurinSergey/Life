/* Обработчик панели */

MACRO EventPanel(pn, CMD, ID, KEY)

    if (CMD == DLG_INIT)
        return InitPanel(pn, CMD, ID, KEY);
    end;
    if (CMD == DLG_KEY)
        if(KEY == 317) /* F3 */
            SetTimer(mon_panel,UpdateTime,false);
            ListPanel(pn, CMD, ID, KEY);
            ShowReportRunTime(pn);
            SetTimer(mon_panel,UpdateTime);
            UpdateFields(pn);
            return CM_IGNORE;
        elif((KEY == 13) and (ID == FldNumber(pn)-1)) /* ENTER */
            SetFocus(pn,0);
            return CM_IGNORE;
        elif(KEY == 316) /* F2 */
     debugbreak;
            SetTimer(mon_panel,UpdateTime,false);
            ShowReport(pn);
            ShowReportRunTime(pn);
            SetTimer(mon_panel,UpdateTime);
            UpdateFields(pn);
            return CM_IGNORE;
        elif(KEY == 32)  /* Пробел */
            if(pn.rec.oper != "Все")
                pn.rec.oper = "Все";
                ShowReportRunTime(pn);
            end;
            UpdateFields(pn);
            return CM_IGNORE;
        elif(KEY == 319)  /* F5 */
            SetTimer(mon_panel,UpdateTime,false);
            ShowRobots(pn);
            SetTimer(mon_panel,UpdateTime);
        elif(KEY == 320) /* F6 */
            SetTimer(mon_panel,UpdateTime,false);
            ShowUnknownRobots(pn);
            SetTimer(mon_panel,UpdateTime);
        elif(KEY == KEY_CTRL_F)
            SetTimer(mon_panel,UpdateTime,false);
            ExecMacroFile("SearchForOperation.mac","RunSearch");
            SetTimer(mon_panel,UpdateTime);
        elif(KEY == KEY_ALT_P)
            SetTimer(mon_panel,UpdateTime,false);
            ExecMacroFile("RobotsPing.mac","RunCheckRobot");
            SetTimer(mon_panel,UpdateTime);
        end;
    end;
    if(CMD == DLG_TIMER) /* Таймер */
    //debugbreak;
        ShowReportRunTime(pn);
        UpdateFields(pn/*,FldIndex(pn,"rep")*/);
        return CM_IGNORE;		
    end;

END;

debugbreak;
if (rundialog(mon_panel,"EventPanel"))
    exit(1);
else
    exit(1);
end;
