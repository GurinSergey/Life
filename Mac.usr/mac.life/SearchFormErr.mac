macro Is_Digit( str, digit_ )
        var isdigit, digit=digit_;
        var i, n, cur_symb;
        isdigit = true;
        i = 1;
        n = strlen(str);
        while( (isdigit) and (i <= n) )
                cur_symb = SubStr(str, i, 1);
                if(index(digit,cur_symb) == 0)
                        isdigit = false;
                end;
                i = i+1;
        end;
        return isdigit;
end; 
              
