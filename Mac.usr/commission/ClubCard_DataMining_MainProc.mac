 MACRO Main_Unit(DateB, DateE, NeedPrintLog)
  
  DateBegin = DateB;
  DateEnd   = DateE;
  
  initUpdateOperation (DateBegin, DateEnd);  // обновление
  DisconnectCommissions();                   // отключатор
  ConnectComissions();                       // подключатор
  
 
  if (NeedPrintLog)
    PrintReport ();                          // отчет (печать полной таблицы)
    exit (RSL_EXIT_SUCCESS);                 // нормальный человеческий выход (0 = видно печатную форму отчёта)
  end;
 
