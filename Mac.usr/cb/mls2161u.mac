import "name_m.mac";
import "pr2161lib.mac";

private VAR CopyCont = 1;          /* Кол-во копий                         */

MACRO PrintDocument(ncopy: integer):bool
  debugbreak;
  var ReportData = СформироватьОтчетДляСводногоМО();
  CopyCont = ncopy;
  while(CopyCont)
    ReportData.PrintReport();
    CopyCont = CopyCont - 1;
  end;
  return true;

END;