import globals;
import lib_fg;

/**получить БИКи банков группы для макросов комиссий**/
macro getBICsBanks()
   if (_bank.is_EXV)
      return "'046311808', '040702756', '042007755', '041806835'";
   end;

   return string("'"+{MFO_BANK}+"'");
end;