    macro isDataVariable(_value)
        return ((_value.activeNatCur  != 0) or
                (_value.activeCur     != 0) or
                (_value.active        != 0) or
                (_value.passiveNatCur != 0) or
                (_value.passiveCur    != 0) or
                (_value.passive       != 0))
    end;

    /**
     * Формирование массива переменных строк.
     */
    macro setVariableToArray(_value, chapter)
        var firstBalance = SubStr(_value.fieldValue("row").currentAsString, 1, 3);
        var inBalance;
        var inFirstBalance;
        var currentValue;

        m_poolVariable.moveFirst();

        while (m_poolVariable.moveNext())
            currentValue = m_poolVariable.getCurrentItem();

            inBalance      = ternary(index(currentValue.balances2, _value.fieldValue("row").currentAsString) > 0, true, false);
            inFirstBalance = ternary(index(currentValue.balances1, SubStr(_value.fieldValue("row").currentAsString, 1, 3)) > 0, true, false);

            /* note: проверяем: либо по БС первого порядка, либо по БС второго порядка */
            if (inBalance OR inFirstBalance)
                if (currentValue.sign == BALANCE_SUM_A_PLUS_P)
                    currentValue.activeNatCur  = currentValue.activeNatCur + NVL(_value.fieldValue("activeNatCur").exact, 0) + NVL(_value.fieldValue("passiveNatCur").exact, 0);
                    currentValue.activeCur     = currentValue.activeCur    + NVL(_value.fieldValue("activeCur").exact, 0) + NVL(_value.fieldValue("passiveCur").exact, 0);
                    currentValue.active        = currentValue.active       + NVL(_value.fieldValue("active").exact, 0) + NVL(_value.fieldValue("passive").exact, 0);
                elif (currentValue.sign == BALANCE_SUM)
                    currentValue.activeNatCur  = currentValue.activeNatCur + NVL(_value.fieldValue("activeNatCur").exact, 0);
                    currentValue.activeCur     = currentValue.activeCur    + NVL(_value.fieldValue("activeCur").exact, 0);
                    currentValue.active        = currentValue.active       + NVL(_value.fieldValue("active").exact, 0);
                    currentValue.passiveNatCur = currentValue.passiveNatCur+ NVL(_value.fieldValue("passiveNatCur").exact, 0);
                    currentValue.passiveCur    = currentValue.passiveCur   + NVL(_value.fieldValue("passiveCur").exact, 0);
                    currentValue.passive       = currentValue.passive      + NVL(_value.fieldValue("passive").exact, 0);
                    if (currentValue.row == "1342")
                        currentValue.activeNatCur  = currentValue.activeNatCur + currentValue.passiveNatCur;
                        currentValue.activeCur     = currentValue.activeCur    + currentValue.passiveCur   ;
                        currentValue.active        = currentValue.active       + currentValue.passive      ;
                    end;
                elif (currentValue.sign == BALANCE_SUM_P_MINUS_A)
                    currentValue.activeNatCur  = 0.0;
                    currentValue.activeCur     = 0.0;
                    currentValue.active        = 0.0;
                    currentValue.passiveNatCur = currentValue.passiveNatCur+ NVL(_value.fieldValue("passiveNatCur").exact, 0) - NVL(_value.fieldValue("activeNatCur").exact, 0);
                    currentValue.passiveCur    = currentValue.passiveCur   + NVL(_value.fieldValue("passiveCur").exact, 0)    - NVL(_value.fieldValue("activeCur").exact, 0);
                    currentValue.passive       = currentValue.passive      + NVL(_value.fieldValue("passive").exact, 0)       - NVL(_value.fieldValue("active").exact, 0);
                elif (currentValue.sign == BALANCE_SUM_A_MINUS_P)
                    if ((firstBalance == "329") AND (currentValue.row == "1068"))    /* note: частный случай */
                        currentValue.activeNatCur  = currentValue.activeNatCur + NVL(_value.fieldValue("activeNatCur").exact, 0);
                        currentValue.activeCur     = currentValue.activeCur    + NVL(_value.fieldValue("activeCur").exact, 0);
                        currentValue.active        = currentValue.active       + NVL(_value.fieldValue("active").exact, 0);
                        currentValue.passiveNatCur = 0.0;
                        currentValue.passiveCur    = 0.0;
                        currentValue.passive       = 0.0;
                    else
                        currentValue.activeNatCur  = currentValue.activeNatCur + NVL(_value.fieldValue("activeNatCur").exact, 0) - NVL(_value.fieldValue("passiveNatCur").exact, 0);
                        currentValue.activeCur     = currentValue.activeCur    + NVL(_value.fieldValue("activeCur").exact, 0)    - NVL(_value.fieldValue("passiveCur").exact, 0);
                        currentValue.active        = currentValue.active       + NVL(_value.fieldValue("active").exact, 0)       - NVL(_value.fieldValue("passive").exact, 0);
                        currentValue.passiveNatCur = 0.0;
                        currentValue.passiveCur    = 0.0;
                        currentValue.passive       = 0.0;
                    end;
                elif (currentValue.sign == BALANCE_SUM_P_MINUS_A2)
                    currentValue.activeNatCur  = 0.0;
                    currentValue.activeCur     = 0.0;
                    currentValue.active        = 0.0;
                    if (inFirstBalance)
                        currentValue.passiveNatCur = currentValue.passiveNatCur+ NVL(_value.fieldValue("passiveNatCur").exact, 0) - NVL(_value.fieldValue("activeNatCur").exact, 0);
                        currentValue.passiveCur    = currentValue.passiveCur   + NVL(_value.fieldValue("passiveCur").exact, 0)    - NVL(_value.fieldValue("activeCur").exact, 0);
                        currentValue.passive       = currentValue.passive      + NVL(_value.fieldValue("passive").exact, 0)       - NVL(_value.fieldValue("active").exact, 0);
                    end;
                    if (inBalance)
                        currentValue.passiveNatCur = currentValue.passiveNatCur+ NVL(_value.fieldValue("passiveNatCur").exact, 0);
                        currentValue.passiveCur    = currentValue.passiveCur   + NVL(_value.fieldValue("passiveCur").exact, 0);
                        currentValue.passive       = currentValue.passive      + NVL(_value.fieldValue("passive").exact, 0);
                    end;
                elif (currentValue.sign == BALANCE_SUM_A)
                    currentValue.activeNatCur  = currentValue.activeNatCur + NVL(_value.fieldValue("activeNatCur").exact, 0);
                    currentValue.activeCur     = currentValue.activeCur    + NVL(_value.fieldValue("activeCur").exact, 0);
                    currentValue.active        = currentValue.active       + NVL(_value.fieldValue("active").exact, 0);
                    currentValue.passiveNatCur = 0.0;
                    currentValue.passiveCur    = 0.0;
                    currentValue.passive       = 0.0;
                elif (currentValue.sign == BALANCE_SUM_A_MINUS_P2)
                    if (inFirstBalance)
                        currentValue.activeNatCur = currentValue.activeNatCur+ NVL(_value.fieldValue("activeNatCur").exact, 0);
                        currentValue.activeCur    = currentValue.activeCur   + NVL(_value.fieldValue("activeCur").exact, 0);
                        currentValue.active       = currentValue.active      + NVL(_value.fieldValue("active").exact, 0);
                    end;
                    if (inBalance)
                        currentValue.activeNatCur = currentValue.activeNatCur - NVL(_value.fieldValue("passiveNatCur").exact, 0);
                        currentValue.activeCur    = currentValue.activeCur    - NVL(_value.fieldValue("passiveCur").exact, 0);
                        currentValue.active       = currentValue.active       - NVL(_value.fieldValue("passive").exact, 0);
                    end;
                    currentValue.passiveNatCur  = 0.0;
                    currentValue.passiveCur     = 0.0;
                    currentValue.passive        = 0.0;
                elif (currentValue.sign == BALANCE_SUB)
                    if (inBalance)  /* note: т.к. только по БС второго порядка */
                        currentValue.activeNatCur  = currentValue.activeNatCur + NVL(_value.fieldValue("activeNatCur").exact, 0);
                        currentValue.activeCur     = currentValue.activeCur    + NVL(_value.fieldValue("activeCur").exact, 0);
                        currentValue.active        = currentValue.active       + NVL(_value.fieldValue("active").exact, 0);
                        currentValue.passiveNatCur = currentValue.passiveNatCur+ NVL(_value.fieldValue("passiveNatCur").exact, 0);
                        currentValue.passiveCur    = currentValue.passiveCur   + NVL(_value.fieldValue("passiveCur").exact, 0);
                        currentValue.passive       = currentValue.passive      + NVL(_value.fieldValue("passive").exact, 0);
                    else
                        currentValue.activeNatCur  = currentValue.activeNatCur - NVL(_value.fieldValue("activeNatCur").exact, 0);
                        currentValue.activeCur     = currentValue.activeCur    - NVL(_value.fieldValue("activeCur").exact, 0);
                        currentValue.active        = currentValue.active       - NVL(_value.fieldValue("active").exact, 0);
                        currentValue.passiveNatCur = currentValue.passiveNatCur- NVL(_value.fieldValue("passiveNatCur").exact, 0);
                        currentValue.passiveCur    = currentValue.passiveCur   - NVL(_value.fieldValue("passiveCur").exact, 0);
                        currentValue.passive       = currentValue.passive      - NVL(_value.fieldValue("passive").exact, 0);
                    end;
            /* note: строка 333 - формирование по всем балансовым счетам второго порядка  */
                elif ((currentValue.sign == BALANCE_SUM_ALL_CHAPTER_1) and (chapter == 1))
                currentValue.activeNatCur  = currentValue.activeNatCur + NVL(_value.fieldValue("activeNatCur").exact, 0);
                currentValue.activeCur     = currentValue.activeCur    + NVL(_value.fieldValue("activeCur").exact, 0);
                currentValue.active        = currentValue.active       + NVL(_value.fieldValue("active").exact, 0);
                currentValue.passiveNatCur = currentValue.passiveNatCur+ NVL(_value.fieldValue("passiveNatCur").exact, 0);
                currentValue.passiveCur    = currentValue.passiveCur   + NVL(_value.fieldValue("passiveCur").exact, 0);
                currentValue.passive       = currentValue.passive      + NVL(_value.fieldValue("passive").exact, 0);
            end;
        end;

        end;
    end;

    /**
     * Сохранить значения по строкам.
     * Значения по строкам именуются в формате 4 цифр (добавляется первая единица), для отличия от балансовых счетов
     */
    macro saveVariblesString(_attribute)
        var activeNatCur;
        var activeCur;
        var active;
        var passiveNatCur;
        var passiveCur;
        var passive;

        m_poolVariable.moveFirst();

        while (m_poolVariable.moveNext())
            if (isDataVariable(m_poolVariable.getCurrentItem()))
                activeNatCur  = m_poolVariable.getCurrentItem().activeNatCur;
                activeCur     = m_poolVariable.getCurrentItem().activeCur;
                active        = m_poolVariable.getCurrentItem().active;
                passiveNatCur = m_poolVariable.getCurrentItem().passiveNatCur;
                passiveCur    = m_poolVariable.getCurrentItem().passiveCur;
                passive       = m_poolVariable.getCurrentItem().passive;

                var currentValue = _attribute.addValue();
                currentValue.reset();
                currentValue.fieldValue("row").exact           = m_poolVariable.getCurrentItem().row;

                if (m_poolVariable.getCurrentItem().onlyPositiv)
                    activeNatCur  = ternary(activeNatCur < 0, 0, activeNatCur);
                    activeCur     = ternary(activeCur < 0, 0, activeCur);
                    active        = ternary(active < 0, 0, active);
                    passiveNatCur = ternary(passiveNatCur < 0, 0, passiveNatCur);
                    passiveCur    = ternary(passiveCur < 0, 0, passiveCur);
                    passive       = ternary(passive < 0, 0, passive);
                end;

                currentValue.fieldValue("activeNatCur").exact  = activeNatCur;
                currentValue.fieldValue("activeCur").exact     = activeCur;
                currentValue.fieldValue("active").exact        = active;
                currentValue.fieldValue("passiveNatCur").exact = passiveNatCur;
                currentValue.fieldValue("passiveCur").exact    = passiveCur;
                currentValue.fieldValue("passive").exact       = passive;
                currentValue.recalculateScaled();
            end;
        end;
    end;

    /**
     * Расчет по главе
     * Перебор по балансовым счетам
     */
    macro chapterCalculate(_chapter, _attrAppl8)

        var attrBalance  = TBalanceAttribute("БАЛАНС");
        if (not attrBalance.isCalculated())
            objectFactoryInstance.createReportCalculateController(0).calculate();
            executeSystemOperation(18022);    /* note: Расчет по множеству структурных значений */
        end;

        attrBalance  = TBalanceAttribute("БАЛАНС");

        attrBalance.createBalanceIterator(_chapter, KIND_ALL, true, false);
        attrBalance.sortIteratorByBalance();

        var currentValue;
        var currentFirstBalanceValue;

        var keyValue;
        var lineCompositeValue;

        while (attrBalance.next())
            if (int(attrBalance.getBalance()) != 0)
                    keyValue = _attrAppl8.createKeyValue();
                keyValue.fieldValue("row") = int(attrBalance.getBalance());
                    currentValue = _attrAppl8.findValue(keyValue);
                    if (currentValue == null)
                        currentValue = _attrAppl8.addValue();
                        currentValue.reset();
                    currentValue.fieldValue("row").exact = int(attrBalance.getBalance());
                end;

                currentValue.fieldValue("passiveNatCur").exact  = 0.0;
                currentValue.fieldValue("passiveCur").exact     = 0.0;
                currentValue.fieldValue("passive").exact        = 0.0;
                currentValue.fieldValue("passiveNatCur").exact  = 0.0;
                currentValue.fieldValue("passiveCur").exact     = 0.0;
                currentValue.fieldValue("passive").exact        = 0.0;

                if (attrBalance.getKindBalance() == "А")      //Активный счет
                    currentValue.fieldValue("activeNatCur").exact  = attrBalance.getOutRestNatCurActive().exact;
                    currentValue.fieldValue("activeCur").exact     = attrBalance.getOutRestCurActive().exact;
                    currentValue.fieldValue("active").exact        = attrBalance.getOutRestActive().exact;
                elif(attrBalance.getKindBalance() == "П")     //Пассивный счет
                    currentValue.fieldValue("passiveNatCur").exact = attrBalance.getOutRestNatCurPassive().exact;
                    currentValue.fieldValue("passiveCur").exact    = attrBalance.getOutRestCurPassive().exact;
                    currentValue.fieldValue("passive").exact       = attrBalance.getOutRestPassive().exact;
                else                                          //Не имеющие типа, отображаются по активу
                    currentValue.fieldValue("activeNatCur").exact  = attrBalance.getOutRestNatCurActive().exact - attrBalance.getOutRestNatCurPassive().exact;
                    currentValue.fieldValue("activeCur").exact     = attrBalance.getOutRestCurActive().exact    - attrBalance.getOutRestCurPassive().exact;
                    currentValue.fieldValue("active").exact        = attrBalance.getOutRestActive().exact       - attrBalance.getOutRestPassive().exact;
                    end;

                    currentValue.recalculateScaled();

                if (strLen(attrBalance.getBalance()) == 5)
                    /* note: сформировать значение переменной по строкам */
                    setVariableToArray(currentValue, _chapter);
            end;
        end;
        end;

    end;

    macro calculateSummaryRow(applicationAttribute, summaryRow : string, rows : String)
        var keyValue = applicationAttribute.createKeyValue();
        var currentAttribute;
        var summaryRowAttribute;

        var currentRow;
        var str = rows;
        var i;

        if (summaryRow == "")
            return;
        end;

        summaryRowAttribute = applicationAttribute.addValue();
        summaryRowAttribute.reset();

        while (str != "")
            i = index(str, ",");
            if (i > 0)
                currentRow = trim(SubStr(str, 1, i - 1));
                str = subStr(str, i + 1);
            else
                currentRow = trim(str);
                str = "";
            end;


            if (currentRow != "")
                keyValue.fieldValue("row") = currentRow;
                currentAttribute = applicationAttribute.findValue(keyValue);

                summaryRowAttribute.fieldValue("row").exact = summaryRow;

                if (currentAttribute != NULL)
                    summaryRowAttribute.fieldValue("activeNatCur").exact  = summaryRowAttribute.fieldValue("activeNatCur").exact  + currentAttribute.fieldValue("activeNatCur").exact;
                    summaryRowAttribute.fieldValue("activeCur").exact     = summaryRowAttribute.fieldValue("activeCur").exact     + currentAttribute.fieldValue("activeCur").exact;
                    summaryRowAttribute.fieldValue("active").exact        = summaryRowAttribute.fieldValue("active").exact        + currentAttribute.fieldValue("active").exact;
                    summaryRowAttribute.fieldValue("passiveNatCur").exact = summaryRowAttribute.fieldValue("passiveNatCur").exact + currentAttribute.fieldValue("passiveNatCur").exact;
                    summaryRowAttribute.fieldValue("passiveCur").exact    = summaryRowAttribute.fieldValue("passiveCur").exact    + currentAttribute.fieldValue("passiveCur").exact;
                    summaryRowAttribute.fieldValue("passive").exact       = summaryRowAttribute.fieldValue("passive").exact       + currentAttribute.fieldValue("passive").exact;
                end;
            end;
        end;

        summaryRowAttribute.recalculateScaled();
    end;

    macro execute()
        var i = 1;
        var attrAppl8  = TBalanceAttribute("ПРИЛОЖЕНИЕ8").getCurrentValue();

        fillPoolVariable();

        attrAppl8.removeAllValues();
        rcbApplication.transactionManager.commit();

