class TApplication13CalculateController()

    private macro isSpodCalculated(attrSpod : TBalanceAttribute) : Bool
        var it = attrSpod.getCurrentValue().createValueIterator();
        return it.count() != 0;
    end;

    macro chapterCalculate(chapter, attrAppl13)
        var attrBalance = TBalanceAttribute("БАЛАНС");
        var attrSpod    = TBalanceAttribute("СПОД");
        var attrBalanceForInRest;
        var inRestSource; // Способ получения входящих остатков:
                          // 1 - Исходящие остатки текущего отчета;
                          // 2 - Входящие остатки текущего отчета;
                          // 3 - Исходящие остатки отчета с ДООП = 31.12.ГГГГ, где ГГГГ = ГГГГ -1 из ДООП

        var currentValue;
        var currentFirstBalanceValue;
        var spodValue;
        var attrValue;
        var wasRecalculated = false;
        var currentBalance = "";

        if ( balanceGlobal().getRcbReport().context.period.kind == RCB_PK_YEAR) // Период расчета - год
            inRestSource = 1;
        else
            var d, m, y;
            dateSplit(balanceGlobal().getRcbReport().context.period.beginDate(), d, m, y);

            if ((d == 1) and (m == 1)) //Начало периода - 1 января
                inRestSource = 2;
            else
                inRestSource = 3;

                var restDate = date(31, 12, y -1);
                var currentReport = balanceGlobal().getRcbReport();

                var context = RcbReportContext(RcbPeriod(restDate, restDate),
                                           currentReport.context.departmentCode,
                                           currentReport.context.issueMode,
                                           currentReport.context.organizationStructure,
                                           currentReport.context.isSummaryMode);

                var balanceReportForInRest = RcbApplication().objectFactory.createReport(currentReport.form.id, context).createBalanceReport(RCB_CBR_IS_ANY_BEGINNING_DATE);

                if (not balanceReportForInRest.isCalculated())
                    msgBox("Отсутствует рассчитанная форма \"Балансовые счета\" за " + String(restDate:f));

                    УстановитьФлагВозврата(STOP_PROC_FLG);

                    exit(1);
                end;

                attrBalanceForInRest = TBalanceAttribute("БАЛАНС", balanceReportForInRest);
            end;
        end;

        if (not attrBalance.isCalculated())
            objectFactoryInstance.createReportCalculateController(0).calculate();
            wasRecalculated = true;
        end;

        if (not isSpodCalculated(attrSpod))
            calculateSpod();
            wasRecalculated = true;
        end;

        if (wasRecalculated)
            executeSystemOperation(18022);    /* note: Расчет по множеству структурных значений */
            rcbApplication.transactionManager.commit();
        end;

        // Расчет Актива
        attrBalance.createBalanceIterator(chapter, KIND_ACTIVE, true, false);
        attrBalance.sortIteratorByPart();

        while (attrBalance.next())
            if ((currentBalance != SubStr(attrBalance.getBalance(), 1, 3)) OR (strLen(attrBalance.getBalance()) == 3))

                if (strLen(attrBalance.getBalance()) == 3)
                    currentFirstBalanceValue.fieldValue("name").exact = attrBalance.getBalanceName();
                    continue;
                end;

                currentBalance = SubStr(attrBalance.getBalance(), 1, 3);

                currentFirstBalanceValue = attrAppl13.addValue();

                currentFirstBalanceValue.reset();

                currentFirstBalanceValue.fieldValue("balance").exact = currentBalance;


             end;

            currentValue = currentFirstBalanceValue.addValue();
            currentValue.reset();
            currentValue.fieldValue("balance").exact      = attrBalance.getBalance();
            currentValue.fieldValue("name").exact         = attrBalance.getBalanceName();
            currentValue.fieldValue("part").exact         = attrBalance.getPart().exact;
            currentValue.fieldValue("kind").exact         = attrBalance.getKindBalance();
            currentValue.fieldValue("isBwp").exact        = ternary(attrBalance.isBwp(), 1, 0);
            currentValue.fieldValue("chapter").exact      = attrBalance.getChapter();

            if (inRestSource == 1)
                currentValue.fieldValue("restIn").exact       = attrBalance.getOutRest().exact;
                currentValue.fieldValue("restInNatCur").exact = attrBalance.getOutRestNatCur().exact;
                currentValue.fieldValue("restInCur").exact    = attrBalance.getOutRestCur().exact;
            elif (inRestSource == 2)
            currentValue.fieldValue("restIn").exact       = attrBalance.getInRest().exact;
            currentValue.fieldValue("restInNatCur").exact = attrBalance.getInRestNatCur().exact;
            currentValue.fieldValue("restInCur").exact    = attrBalance.getInRestCur().exact;
            else
                attrValue = attrBalanceForInRest.getBalanceAttribute(attrBalance.getChapter(), attrBalance.getBalance(), false);

                if (attrValue != NULL)
                currentValue.fieldValue("restIn").exact       = attrBalanceForInRest.getOutRest().exact;
                currentValue.fieldValue("restInNatCur").exact = attrBalanceForInRest.getOutRestNatCur().exact;
                currentValue.fieldValue("restInCur").exact    = attrBalanceForInRest.getOutRestCur().exact;
                else
                    currentValue.fieldValue("restIn").exact       = 0;
                    currentValue.fieldValue("restInNatCur").exact = 0;
                    currentValue.fieldValue("restInCur").exact    = 0;
                end;
            end;

            spodValue = attrSpod.getBalanceAttribute(NULL, attrBalance.getBalance(), false);

            if (spodValue != NULL)
                currentValue.fieldValue("debetNatCur").exact  = spodValue.fieldValue("nextYearDebetRouble").exact;
                currentValue.fieldValue("debetCur").exact     = spodValue.fieldValue("nextYearDebetCover").exact;
                currentValue.fieldValue("debet").exact        = spodValue.fieldValue("nextYearDebetRouble").exact + spodValue.fieldValue("nextYearDebetCover").exact;

                currentValue.fieldValue("creditNatCur").exact = spodValue.fieldValue("nextYearCreditRouble").exact;
                currentValue.fieldValue("creditCur").exact    = spodValue.fieldValue("nextYearCreditCover").exact;
                currentValue.fieldValue("credit").exact       = spodValue.fieldValue("nextYearCreditRouble").exact + spodValue.fieldValue("nextYearCreditCover").exact;
            else
                currentValue.fieldValue("debetNatCur").exact  = 0;
                currentValue.fieldValue("debetCur").exact     = 0;
                currentValue.fieldValue("debet").exact        = 0;

                currentValue.fieldValue("creditNatCur").exact = 0;
                currentValue.fieldValue("creditCur").exact    = 0;
                currentValue.fieldValue("credit").exact       = 0;
            end;

            currentValue.fieldValue("restOut").exact       = currentValue.fieldValue("restIn").exact       + currentValue.fieldValue("debet").exact       - currentValue.fieldValue("credit").exact;
            currentValue.fieldValue("restOutNatCur").exact = currentValue.fieldValue("restInNatCur").exact + currentValue.fieldValue("debetNatCur").exact - currentValue.fieldValue("creditNatCur").exact;
            currentValue.fieldValue("restOutCur").exact    = currentValue.fieldValue("restInCur").exact    + currentValue.fieldValue("debetCur").exact    - currentValue.fieldValue("creditCur").exact;

            currentValue.recalculateScaled();
       end;

        // Расчет Пассива
        attrBalance.createBalanceIterator(chapter, KIND_PASSIVE, true, false);
        attrBalance.sortIteratorByPart();

        while (attrBalance.next())
            if ((currentBalance != SubStr(attrBalance.getBalance(), 1, 3)) OR (strLen(attrBalance.getBalance()) == 3))

                if (strLen(attrBalance.getBalance()) == 3)
                    currentFirstBalanceValue.fieldValue("name").exact = attrBalance.getBalanceName();
                    continue;
                end;

                currentBalance = SubStr(attrBalance.getBalance(), 1, 3);

                currentFirstBalanceValue = attrAppl13.addValue();

                currentFirstBalanceValue.reset();

                currentFirstBalanceValue.fieldValue("balance").exact = currentBalance;
            end;

            currentValue = currentFirstBalanceValue.addValue();
            currentValue.reset();
            currentValue.fieldValue("balance").exact      = attrBalance.getBalance();
            currentValue.fieldValue("name").exact         = attrBalance.getBalanceName();
            currentValue.fieldValue("part").exact         = attrBalance.getPart().exact;
            currentValue.fieldValue("kind").exact         = attrBalance.getKindBalance();
            currentValue.fieldValue("chapter").exact      = attrBalance.getChapter();
/* merge */
            currentValue.fieldValue("isBwp").exact        = ternary(attrBalance.isBwp(), 1, 0); 
/* merge */

            if (inRestSource == 1)
                currentValue.fieldValue("restIn").exact       = attrBalance.getOutRest().exact;
                currentValue.fieldValue("restInNatCur").exact = attrBalance.getOutRestNatCur().exact;
                currentValue.fieldValue("restInCur").exact    = attrBalance.getOutRestCur().exact;
            elif (inRestSource == 2)
            currentValue.fieldValue("restIn").exact       = attrBalance.getInRest().exact;
            currentValue.fieldValue("restInNatCur").exact = attrBalance.getInRestNatCur().exact;
            currentValue.fieldValue("restInCur").exact    = attrBalance.getInRestCur().exact;
            else
                attrValue = attrBalanceForInRest.getBalanceAttribute(attrBalance.getChapter(), attrBalance.getBalance(), false);

                if (attrValue != NULL)
                currentValue.fieldValue("restIn").exact       = attrBalanceForInRest.getOutRest().exact;
                currentValue.fieldValue("restInNatCur").exact = attrBalanceForInRest.getOutRestNatCur().exact;
                currentValue.fieldValue("restInCur").exact    = attrBalanceForInRest.getOutRestCur().exact;
                else
                    currentValue.fieldValue("restIn").exact       = 0;
                    currentValue.fieldValue("restInNatCur").exact = 0;
                    currentValue.fieldValue("restInCur").exact    = 0;
                end;
            end;

            spodValue = attrSpod.getBalanceAttribute(NULL, attrBalance.getBalance(), false);

            if (spodValue != NULL)
                currentValue.fieldValue("debetNatCur").exact  = spodValue.fieldValue("nextYearDebetRouble").exact;
                currentValue.fieldValue("debetCur").exact     = spodValue.fieldValue("nextYearDebetCover").exact;
                currentValue.fieldValue("debet").exact        = spodValue.fieldValue("nextYearDebetRouble").exact + spodValue.fieldValue("nextYearDebetCover").exact;

                currentValue.fieldValue("creditNatCur").exact = spodValue.fieldValue("nextYearCreditRouble").exact;
                currentValue.fieldValue("creditCur").exact    = spodValue.fieldValue("nextYearCreditCover").exact;
                currentValue.fieldValue("credit").exact       = spodValue.fieldValue("nextYearCreditRouble").exact + spodValue.fieldValue("nextYearCreditCover").exact;
            else
                currentValue.fieldValue("debetNatCur").exact  = 0;
                currentValue.fieldValue("debetCur").exact     = 0;
                currentValue.fieldValue("debet").exact        = 0;

                currentValue.fieldValue("creditNatCur").exact = 0;
                currentValue.fieldValue("creditCur").exact    = 0;
                currentValue.fieldValue("credit").exact       = 0;
            end;

            currentValue.fieldValue("restOut").exact       = currentValue.fieldValue("restIn").exact       - currentValue.fieldValue("debet").exact       + currentValue.fieldValue("credit").exact;
            currentValue.fieldValue("restOutNatCur").exact = currentValue.fieldValue("restInNatCur").exact - currentValue.fieldValue("debetNatCur").exact + currentValue.fieldValue("creditNatCur").exact;
            currentValue.fieldValue("restOutCur").exact    = currentValue.fieldValue("restInCur").exact    - currentValue.fieldValue("debetCur").exact    + currentValue.fieldValue("creditCur").exact;

            currentValue.recalculateScaled();
       end;
    end;

    macro execute()
        var i = 1;
        var attrAppl13  = TBalanceAttribute("ПРИЛОЖЕНИЕ13").getCurrentValue();
        var compositeValue;

        attrAppl13.removeAllValues();
        rcbApplication.transactionManager.commit();

        while (i < 6)
            compositeValue = attrAppl13.addValue();
            compositeValue.reset();

            compositeValue.fieldValue("balance").exact = "ГЛАВА" + i;
            compositeValue.fieldValue("chapter").exact = i;


            chapterCalculate(i, compositeValue);
            i = i + 1;
        end;

        rcbApplication.transactionManager.commit();
    end;

end;

